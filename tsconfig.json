{
  "compilerOptions": {
    "target": "ES6",
    "strict": true,
    "module": "CommonJS",
    "sourceMap": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@plugins/*": ["plugins/*"],
      "@pageObjects/*": ["supports/pageObject/*"],
      "@pages/*": ["supports/page/*"],
      "@util/*": ["supports/util/*"],
    }
  }
}
